{
    "version": 2,
    "builds": [
      {"src": "*.js", "use": "@vercel/node"}
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ],
    "rewrites": [
      {
        "source": "/admin/dashboard/upload",
        "destination": "/index.html"
      },
      {
        "source": "/admin/dashboard/manage",
        "destination": "/index.html"
      },
      {
        "source": "/admin/dashboard/edit-book/:id",
        "destination": "/index.html"
      },
      {
        "source": "/(.*)",
        "destination": "/index.html"
      }
    ]
  }